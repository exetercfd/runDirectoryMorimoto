FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box ( -1.0 0.005 -1.0 ) ( 1.0 0.01713246 1.0 );
        fieldValues
        (
            volScalarFieldValue alpha.particles 0.510
        );
    }    
    
);


